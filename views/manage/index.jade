!= partial('topnavbar.jade')
	.container-fluid
		.row-fluid
			.span2
				!= partial('sidebar.jade')
			.span10
					ul(data-bind="foreach: folders").nav.nav-tabs
						li(data-bind="css:{active: $data == $root.chosenFolderId()}, click: $root.goToFolder")
							a(data-bind="text: $data")
			.span10			
				h2 
					span(data-bind="text: $root.chosenFolderId()+' '")
					| tickets
				br
				table.table.table-bordered.table-striped
						thead
								tr
									th age
									th subject									
									th from
									th id
						tbody(data-bind="foreach: chosenFolderData")
							tr
								td
									span(data-bind="text: age")
								td
									span(data-bind="text: subject")
								td
									span(data-bind="text: from")
								td
									span(data-bind="text: _id")







script(src='/js/manage-viewmodel.js')



